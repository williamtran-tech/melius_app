{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "icon": "./assets/images/Logo.png",
    "splash": {
      "image": "./assets/images/verticalLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-localization", "expo-camera"],
    "ios": {
      "infoPlist": {
        "NSFaceIDUsageDescription": "Authenticate with Face ID",
        "NSCameraUsageDescription": "Allow MeliusChat to access your camera."
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": ["USE_BIOMETRIC", "CAMERA"]
    },
    "extra": {
      "eas": {
        "projectId": "7ebea6ba-b750-400f-a2ea-7fe8dfd9b576"
      }
    }
  },
  "name": "client"
}
