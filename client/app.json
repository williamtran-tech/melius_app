{
  "expo": {
    "name": "client",
    "scheme": "meliusapp",
    "slug": "client",
    "version": "1.0.0",
    "icon": "./assets/images/Logo.png",
    "splash": {
      "image": "./assets/images/verticalLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-localization", "expo-camera", "expo-media-library"],
    "ios": {
      "infoPlist": {
        "NSFaceIDUsageDescription": "Authenticate with Face ID",
        "NSCameraUsageDescription": "Allow MeliusChat to access your camera.",
        "NSPhotoLibraryUsageDescription": "We need access to your photos to upload them."
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "USE_BIOMETRIC",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "7ebea6ba-b750-400f-a2ea-7fe8dfd9b576"
      }
    }
  },
  "name": "client"
}
